<fieldset id="coming">
  <legend>
    Предстоящие игры (<%= Game.notstarted.count  %>)
  </legend>
  <% Game.notstarted.each do |game | %>
    Предстоит игра <b><%= link_to game.name, resource(game) %></b> |
    <% if game.created_by?(current_user) %>
      <b>Вы автор игры</b>
    <% else %>
      <% @game_entry = GameEntry.of(@current_user,game)%>
      <%= partial 'shared/game_entry_controls', :game_entry => @game_entry,  :game => game, :user => @current_user %>
      <%= link_to "Войти!", resource(game) %>
    <% end %>
    <br/>
  <% end %>
</fieldset>
